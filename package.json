{
  "name": "@ikepu-tp/react-mvc",
  "type": "module",
  "version": "0.8.1",
  "description": "React MVC is a library for supporting MVC-like implementations in JavaScript projects.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier -w \"./src/**/*.{js,css,ts}\"",
    "build": "yarn format && vite build && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikepu-tp/react-mvc.git"
  },
  "keywords": [
    "react",
    "react.js",
    "react-mvc",
    "mvc"
  ],
  "author": "ikepu-tp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ikepu-tp/react-mvc/issues"
  },
  "homepage": "https://github.com/ikepu-tp/react-mvc#readme",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./Model": {
      "require": "./dist/cjs/Model.js",
      "import": "./dist/esm/Model.js",
      "types": "./dist/esm/Model.d.ts"
    },
    "./Url": {
      "require": "./dist/cjs/Url.js",
      "import": "./dist/esm/Url.js",
      "types": "./dist/esm/Url.d.ts"
    },
    "./Send": {
      "require": "./dist/cjs/Send.js",
      "import": "./dist/esm/Send.js",
      "types": "./dist/esm/Send.d.ts"
    },
    "./Function": {
      "require": "./dist/cjs/Function.js",
      "import": "./dist/esm/Function.js",
      "types": "./dist/esm/Function.d.ts"
    }
  },
  "publishConfig": {
    "directory": "dist"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
