{
  "name": "@ikepu-tp/react-mvc",
  "type": "module",
  "version": "0.4.0",
  "description": "React MVC is a library for supporting MVC-like implementations in JavaScript projects.",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier -w \"./src/**/*.{js,css,ts}\"",
    "build": "yarn format && vite build && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikepu-tp/react-mvc.git"
  },
  "keywords": [
    "react",
    "react.js",
    "react-mvc",
    "mvc"
  ],
  "author": "ikepu-tp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ikepu-tp/react-mvc/issues"
  },
  "homepage": "https://github.com/ikepu-tp/react-mvc#readme",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    },
    "./Model": {
      "require": "./dist/Model/index.cjs.js",
      "import": "./dist/Model/index.es.js",
      "types": "./dist/Model/index.d.ts"
    },
    "./Url": {
      "require": "./dist/Url/index.cjs.js",
      "import": "./dist/Url/index.es.js",
      "types": "./dist/Url/index.d.ts"
    },
    "./Send": {
      "require": "./dist/Send/index.cjs.js",
      "import": "./dist/Send/index.es.js",
      "types": "./dist/Send/index.d.ts"
    },
    "./functions": {
      "require": "./dist/functions/index.cjs.js",
      "import": "./dist/functions/index.es.js",
      "types": "./dist/functions/index.d.ts"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "prettier": "^3.0.3"
  }
}
